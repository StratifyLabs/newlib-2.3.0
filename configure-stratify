../configure --target=arm-none-eabi --enable-multilib   --disable-werror   --with-gnu-as   --with-gnu-ld   --prefix=/usr/local/StratifyLabs   --disable-newlib-supplied-syscalls   --disable-nls   --enable-target-optspace   --disable-libssp   --enable-newlib-reent-small   --enable-newlib-multithread   --disable-libgloss
make CFLAGS_FOR_TARGET="-fomit-frame-pointer -fsingle-precision-constant -ffunction-sections -DSIGNAL_PROVIDED -DMALLOC_PROVIDED -DHAVE_SYSTEM -DHAVE_FCNTL -DHAVE_RENAME -D__BUFSIZ__=64 -D__IEEE_LITTLE_ENDIAN -D__IEEE_BYTES_LITTLE_ENDIAN -D__StratifyOS__"